<!doctype html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>OCS - Battle Mode</title>
<% include partials/head %>
<link rel="stylesheet" href="/battlemoderoom.css">
<link rel="stylesheet" href="/typography.css">
</head>
<body>
  <div class="backgroundDiv"></div>
   <div class="row">
	  <div class="column left ">
		<div class="playerContainer">
			<div class="BanLeft">
				<h3></h3>
			</div>
			<div class="PlayerInfo">
					<div class="Row-stats" style="border-bottom: 3px groove #222;">
						<img class="PlayersIcone Column-stats"  align="left">
						<div class="PlayerName Column-stats display2" style="text-align: left;	width:60%;"></div>
				    </div>
				<div class="flex-row">
					<div class="Row-stats" style="border-bottom: 3px groove #222;">
							<div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Routing</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
							 <div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Execution</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
							 <div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Adaptation</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
					</div>
				</div>
			</div>
		</div>
	  </div>
	  <div class="column middle ">
		<div class="Timer">
			<h3 class="TimerText display3"></h3>
		</div>
		<div class="flex-row">
			<div class="content flex flex-center">
			<div class="container">
			  <div class="challenges">
				  <% data.mode.forEach((challenge, i) => { %>
					<div class="challenge-card noselect c4 flex" style="background-image: url(<%= challenge.background %>)">
					  <div class="gradient flex-grow">
						<div class="flex-row">
						  <div class="number"><%= i + 1 %></div>
						  <div class="flex-grow">
							<div class="date secondary-dark"></div>
							<div class="headline name flex flex-center"><%- challenge.title %></div>
						  </div>
						</div>
						<div class="flex-row space-around">
						  <div class="flex flex-grow flex-center"></div>
						</div>
					  </div>
					</div>
				  <% }); %>
				</div>
			  </div>
			</div>
		</div>
		<div class="state">
			<h3 class="display2"> </h3>
		</div>
	  </div>

	  <div class="column right ">
		<div class="playerContainer">
			<div class="BanRight">
				<h3></h3>
			</div>
			<div class="PlayerInfo">
					<div class="Row-stats" style="border-bottom: 3px groove #222;">
						<div class="PlayerName Column-stats display3" style="text-align: right;width:60%;"></div>
						<img alt="" class="PlayersIcone Column-stats"  align="right">
				    </div>
				<div class="flex-row style="position: relative">
					<div class="Row-stats" style="border-bottom: 3px groove #222;">
							<div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Routing</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
							 <div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Execution</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
							 <div class="Row-stats">
								  <div class="Column-stats">
									<h5 class="StatsText" style="color:white; text-align:center;">Adaptation</h5>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progress-bar-striped" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progress-bar-striped" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
								  <div class="progress Column-stats">
									<div class="progress-bar" role="progress-bar-striped" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%"></div>
								  </div>
							 </div>
					</div>
				</div>
			</div>
		</div>
	  </div>
  </div>
</div>
</body>
<script>
$(document).ready( function() {

</script>
</html>
